        <div class="panel-body" id="detailsBody" style="padding-top: 20px; overflow-y: scroll;">
        </div>
        <div class="panel-footer" style="height: 42px;">
            <button type="button" id="btn_split" class="btn btn-warning" style="float: right; margin: -5px 5px 5px; display: none;" onclick="Split()">
                <i class="fa fa-expand" aria-hidden="true"></i>&nbsp;拆分 
            </button>
        </div>
    </div>

