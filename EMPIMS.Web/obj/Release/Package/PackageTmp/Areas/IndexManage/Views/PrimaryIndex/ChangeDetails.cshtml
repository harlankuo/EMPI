    <script>
        var keyValue = $.request("keyValue");
        var operation_id = "";

        $(function () {

            initWindow();
            GetChangeRecord();
        });


        $(window).resize(function () {
            initWindow();
        });

        //初始化页面
        function initWindow() {
            var winHeight = $(window).height();
            var winWidth = $(window).width();
            $(".panel").css("height", winHeight - 20);
            $("#rightPannel").css("width", winWidth - 405);
            //$("#detailsBody").css("height", winHeight - 100);
            $(".panel-body").css("height", winHeight - 100);
        }

        //获取主索引变动记录
        function GetChangeRecord() {
            if (!!keyValue) {
                $.ajax({
                    url: "/IndexManage/PrimaryIndex/GetChangeDetails",
                    data: { keyValue: keyValue },
                    dataType: "json",
                    async: true,
                    success: function (data) {
                        var strHtml = "";
                        var typeName = "";

                        if (!!data && data.length > 0) {
                            //strHtml = "<tr><td>变动ID<td><td>变动类型<td><td>变动人<td><td>变动时间<td></tr>";
                            for (var i = 0; i < data.length; i++) {
                                switch (data[i].OPERATION_TYPE) {
                                    case "ADD":
                                        typeName = "新增";
                                        break;
                                    case "UPDATE":
                                        typeName = "修改";
                                        break;
                                    case "MERGE":
                                        typeName = "合并";
                                        break;
                                    case "SPLIT":
                                        typeName = "拆分";
                                        break;
                                    case "POTENTIAL_DUPLICATE":
                                        typeName = "潜在重复";
                                        break;
                                    case "ACTIVE":
                                        typeName = "启用";
                                        break;
                                    case "DISABLE":
                                        typeName = "停用";
                                        break;
                                    default:
                                        typeName = "未定义";
                                        break;

                                }

                                strHtml += "<a href=\"#\" class=\"list-group-item\" style='curosr: pointer;' onclick=\"btn_detail('" + data[i].OPERATION_ID + "','" + data[i].OPERATION_TYPE + "',this)\" class=\"dataRow\">" + "【" + typeName + "】" + " - " + data[i].OPERATE_BY + " - " + data[i].OPERATE_TIME + "</a>";
                            }
                            $("#ChangeRecord").html(strHtml);

                            $("#ChangeRecord a").click(function () {
                                $("#ChangeRecord a").removeClass("active");
                                $(this).addClass("active");
                            });
                            $("#ChangeRecord a").eq(0).click();
                        }
                    }
                });
            }
        }

        //查看详细
        function btn_detail(_id, _type, obj) {
            operation_id = _id;
            $("#changeID").text(" -【" + _id + "】");
            $("#btn_split").hide();
            switch (_type) {

                case "ADD":
                    type_add(_id);
                    break;
                case "UPDATE":
                    type_update(_id);
                    break;
                case "MERGE":
                    {
                        if (IsSplitable()) {
                            $("#btn_split").show();
                        }
                        type_merge_split(_id, _type);
                        break;
                    }
                case "SPLIT":
                    type_merge_split(_id, _type);
                    break;
                case "POTENTIAL_DUPLICATE":
                    type_potentialL_duplicate(_id);
                    break;
                case "ACTIVE":
                    type_merge_split(_id, _type);
                    break;
                case "DISABLE":
                    type_merge_split(_id, _type);
                    break;
                default:
            }
        }

        //新增类型
        function type_add(_id) {
            var strHtml = "";
            if (!!_id) {
                $.ajax({
                    url: "/IndexManage/PrimaryIndex/GetFormJson",
                    data: { keyValue: keyValue },
                    dataType: "json",
                    async: false,
                    success: function (data) {
                        // console.log(data);
                        if (!!data) {
                            for (var key in data) {
                                if (data[key] == null) {
                                    data[key] = "";
                                }
                            }
                            strHtml = baidu.template('add-template', data);
                            $("#detailsBody").html(strHtml);
                        }
                    }
                });
            }

        }

        //修改类型
        function type_update(_id) {
            var strHtml = "";
            var obj = {};
            if (!!_id) {
                $.ajax({
                    url: "/IndexManage/PrimaryIndex/GetOperationEntityJson",
                    data: { keyValue: keyValue, operation_ID: _id },
                    dataType: "text",
                    async: false,
                    success: function (data) {
                        if (!!data) {

                            data = $.parseJSON(data);

                            for (var key in data.BEFORE_UPDATE) {

                                if (data.BEFORE_UPDATE[key] == null) {
                                    data.BEFORE_UPDATE[key] = "";
                                }
                                obj["" + key + "_1"] = data.BEFORE_UPDATE[key];
                            }

                            for (var key in data.AFTER_UPDATE) {
                                if (data.AFTER_UPDATE[key] == null) {
                                    data.AFTER_UPDATE[key] = "";
                                }

                                obj["" + key + "_0"] = data.AFTER_UPDATE[key];
                            }

                            strHtml = baidu.template('edit-template', obj);
                            $("#detailsBody").html(strHtml);
                        }
                    }
                });
            }

        }

        //合并、拆分类型
        function type_merge_split(_id, _type) {

            var strHtml = "";
            var obj = { TableTitle: "合并详情" };
            if (_type == "SPLIT") {
                obj.TableTitle = "拆分详情";
            } else if (_type == "ACTIVE") {
                obj.TableTitle = "启用详情";
            } else if (_type == "DISABLE") {
                obj.TableTitle = "停用详情";
            }
            if (!!_id) {
                $.ajax({
                    url: "/IndexManage/PrimaryIndex/GetOperationEntityJson",
                    data: { keyValue: keyValue, operation_ID: _id },
                    dataType: "text",
                    async: false,
                    success: function (data) {
                        if (!!data) {

                            data = $.parseJSON(data);

                            for (var key in data.MATCH_0) {

                                if (data.MATCH_0[key] == null) {
                                    data.MATCH_0[key] = "";
                                }
                                obj["" + key + "_0"] = data.MATCH_0[key];
                            }

                            for (var key in data.MATCH_1) {
                                if (data.MATCH_1[key] == null) {
                                    data.MATCH_1[key] = "";
                                }
                                obj["" + key + "_1"] = data.MATCH_1[key];
                            }

                            strHtml = baidu.template('split-merge-template', obj);

                            $("#detailsBody").html(strHtml);
                        }
                    }
                });
            }



        }

        //潜在重复
        function type_potentialL_duplicate(_id) {
            var strHtml = "";
            var obj = { TableTitle: "潜在重复详情" };

            if (!!_id) {
                $.ajax({
                    url: "/IndexManage/PrimaryIndex/GetOperationEntityJson",
                    data: { keyValue: keyValue, operation_ID: _id },
                    dataType: "text",
                    async: false,
                    success: function (data) {
                        if (!!data) {

                            data = $.parseJSON(data);

                            for (var key in data.DUPLICATE_0) {

                                if (data.DUPLICATE_0[key] == null) {
                                    data.DUPLICATE_0[key] = "";
                                }
                                obj["" + key + "_0"] = data.DUPLICATE_0[key];
                            }

                            for (var key in data.DUPLICATE_1) {
                                if (data.DUPLICATE_1[key] == null) {
                                    data.DUPLICATE_1[key] = "";
                                }
                                obj["" + key + "_1"] = data.DUPLICATE_1[key];
                            }

                            strHtml = baidu.template('split-merge-template', obj);
                        }
                    }
                });
            }

            $("#detailsBody").html(strHtml);
        }

        //判断是否能拆分
        function IsSplitable() {
            var flag = false;
            $.ajax({
                url: "/IndexManage/PrimaryIndex/IsSplitable",
                type: "post",
                data: { operation_id: operation_id },
                dataType: "json",
                async: false,
                success: function (data) {
                    if (data.state == "error") {
                        flag = false;
                    } else {
                        flag = true;
                    }
                }
            });
            return flag;
        }

        //拆分
        function Split() {
            $.modalConfirm("您确定拆分吗？", function (r) {
                if (r) {
                    $.ajax({
                        url: "/IndexManage/RepeatPotential/Split",
                        type: "post",
                        data: { operation_id: operation_id },
                        dataType: "json",
                        async: false,
                        success: function (data) {
                            $.modalMsg(data.message, data.state);
                            if (data.state == "success") {
                                GetChangeRecord();
                            }
                        }
                    });
                }
            });
        }

    </script>
    <div class="panel panel-default" style="float: left; width: 400px;">
        <div class="panel-heading">变动记录</div>
        <div class="panel-body" style="padding-bottom: 0px; overflow-y: scroll;">
            <div class="list-group" id="ChangeRecord">
            </div>
        </div>
    </div>
    <div class="panel panel-default" id="rightPannel" style="float: left; margin-left: 5px;">
