<?xml version="1.0" encoding="utf-8"?>
<appSettings>
  <!-- ================== 1：开发系统相关配置 ================== -->
  <!-- 登陆提供者模式：Session、Cookie-->
  <add key="LoginProvider" value="Cookie" />
  <!-- 启用系统日志-->
  <add key="IsLog" value="true" />
  <!-- 数据库超时间-->
  <add key="CommandTimeout" value="180" />
  <!--启用IP过滤 -->
  <add key="IsIPFilter" value="false" />
  <!-- ================== 2：系统软件参数配置 ================== -->
  <!-- 软件名称 -->
  <add key="SoftName" value="萨米患者主索引管理系统" />
  <!-- 软件版本 -->
  <add key="Version" value="1.0" />
  <!-- 软件授权码 测试备用 -->
  <add key="LicenceKey" value="83eda07f-ead3-4050-bcee-4f41e57a7e16" />
  <!--后台管理系统地址-->
  <add key="SysMgmt" value="http://127.0.0.1:81/" />
  <!--系统代码-->
  <add key="SystemCode" value="40"/>
  <!--系统编码-->
  <add key="SystemNumber" value="EMPIMS"/>
  <!--主索引注册服务开关:on、off-->
  <add key="IsIndexRegister" value="on"/>
  <!--主索引检索服务开关:on、off-->
  <add key="IsIndexSearch" value="on"/>
  <!--主索引更新推送开关:on、off-->
  <add key="IsIndexUpdatePush" value="on"/>
  <!--主索引更新推送地址-->
  <add key="IndexPushAddress" value="http://10.100.1.111:6850/services/EmpiService.EmpiServiceHttpSoap12Endpoint"/>
  <!--合并、拆分主索引号变更通知开关:on、off-->
  <add key="IsEmpi_IDChangePush" value="on"/>
  <!--合并、拆分主索引号变更通知-->
  <add key="Empi_IDChangePushAddress" value="http://10.100.1.111:6851/indexNOChangeNotification"/>
  <!--备用数据库连接字符串-->
  <add key ="DBConnection" value="YmlsjLvUxDsYX6i/5HoB1KKK3zNPO5BnO2ItMlq61j4="/>
  <!--备用数数据库名称-->
  <add key="MongoDB" value="74Xe82M2jhvd3O3DyNuVcA=="/>
  <!--脏数据字典：用'|'分割-->
  <add key="DirtyDict" value="无|-"/>
  <!--ID_TYPE_CODE对应的身份证号码值-->
  <add key="ID_TYPE_CODE" value="IdentifyNo"/>
</appSettings>